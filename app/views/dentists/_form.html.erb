
<div class="container-fluid">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
<%= form_for(@dentist) do |f| %>
  <% if @dentist.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@dentist.errors.count, "error") %> prohibited this dentist from being saved:</h2>

      <ul>
      <% @dentist.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field form-group">
    <%= f.label :nombres %><br>
    <%= f.text_field :nombre ,:class => 'form-control'%>
  </div>
  <div class="field form-group">
    <%= f.label :apellidos %><br>
    <%= f.text_field :apellido ,:class => 'form-control'%>
  </div>
  <div class="field form-group">
    <%= f.label :fecha_de_nacimiento %><br>
    <%= f.date_select :fecnac , { :start_year => 1960, :end_year => 2015 , :use_month_names => ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre','Diciembre']} ,:class => 'date datetest'%>

  </div>
  <div class="field form-group">
    <%= f.label :fecha_de_alta %><br>
    <%= f.date_select :fecalta, { :start_year => 2000, :end_year => 2015, :use_month_names => ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre','Diciembre'] } ,:class => 'date datetest'%>
  </div>
  <div class="field form-group">
    <%= f.label :direccion %><br>
    <%= f.text_field :direccion ,:class => 'form-control'%>
  </div>
  <div class="field form-group">
    <%= f.label :telefono %><br>
    <%= f.text_field :telefono ,:class => 'form-control'%>
  </div>
  <div class="field form-group">
    <%= f.label :celular %><br>
    <%= f.text_field :celular ,:class => 'form-control'%>
  </div>
  <div class="field form-group">
    <%= f.label :cop %><br>
    <%= f.text_field :cop ,:class => 'form-control'%>
  </div>
  <div class="field form-group">
    <%= f.label :email %><br>
    <%= f.text_field :email ,:class => 'form-control'%>
  </div>
  <div class="field form-group">
    <%= f.label :usuario %><br>
    <%= f.text_field :user ,:class => 'form-control'%>
  </div>
  <div class="actions">
    <%= f.submit "Crear Odontologo" ,:class => 'btn btn-primary'%>
  </div>
<% end %>
<br>